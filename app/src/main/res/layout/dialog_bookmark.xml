<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tv_dialog_bcyclLendNm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="대여소 이름"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_dialog_lnmadr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="주소"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_bcyclLendNm"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_bcyclLendNm" />

        <TextView
            android:id="@+id/tv_dialog_operHm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="시간"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_lnmadr"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_lnmadr" />

        <TextView
            android:id="@+id/tv_dialog_rstde"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="휴무일"
            app:layout_constraintStart_toEndOf="@+id/tv_dialog_operHm"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_lnmadr" />

        <TextView
            android:id="@+id/tv_dialog_chrgeSe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="무료/유료"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_operHm"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_operHm" />

        <TextView
            android:id="@+id/tv_dialog_bcyclUseCharge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="이용요금정보"
            app:layout_constraintStart_toEndOf="@+id/tv_dialog_chrgeSe"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_operHm" />

        <TextView
            android:id="@+id/tv_dialog_airInjector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="공기 주입기 비치"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_chrgeSe"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_chrgeSe" />

        <TextView
            android:id="@+id/tv_dialog_airInjectorYN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Y/N"
            app:layout_constraintStart_toEndOf="@+id/tv_dialog_airInjector"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_chrgeSe" />

        <TextView
            android:id="@+id/tv_dialog_repairStand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="수리대 설치 여부"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_airInjector"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_airInjector" />

        <TextView
            android:id="@+id/tv_dialog_repairStandYN"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Y/N"
            app:layout_constraintStart_toEndOf="@+id/tv_dialog_repairStand"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_airInjector" />

        <TextView
            android:id="@+id/tv_dialog_phoneNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="전화번호"
            app:layout_constraintStart_toStartOf="@id/tv_dialog_repairStand"
            app:layout_constraintTop_toBottomOf="@id/tv_dialog_repairStand" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>